{
  "name": "ganache-core",
  "version": "2.1.6",
  "main": "./index.js",
  "engines": {
    "node": ">=8.9.0"
  },
  "directories": {
    "lib": "./lib"
  },
  "scripts": {
    "build": "npm run build-server && npm run build-provider && npm run build-core",
    "build-server": "webpack-cli --config ./webpack/server.webpack.config.js",
    "build-provider": "webpack-cli --config ./webpack/provider.webpack.config.js",
    "build-core": "webpack-cli --config ./webpack/core.webpack.config.js",
    "prepare": "npm run build",
    "test": "mocha --check-leaks --recursive --globals _scratch,sanitizedData",
    "test-built": "cross-env TEST_BRANCH=built mocha --check-leaks --recursive --globals _scratch,sanitizedData",
    "test-watch": "cross-env TEST_BRANCH=built mocha-webpack --webpack-config \"./webpack/core.webpack.config.js\" --watch --check-leaks --recursive --globals _scratch,sanitizedData"
  },
  "dependencies": {
    "abstract-leveldown": "5.0.0",
    "async": "^2.5.0",
    "bip39": "~2.4.0",
    "bn.js": "4.11.6",
    "cachedown": "^1.0.0",
    "clone": "^2.1.1",
    "eth-sig-util": "^2.0.2",
    "encoding-down": "5.0.4",
    "ethereumjs-account": "~2.0.4",
    "ethereumjs-block": "~1.2.2",
    "ethereumjs-tx": "1.3.4",
    "ethereumjs-util": "^5.2.0",
    "ethereumjs-vm": "2.3.5",
    "ethereumjs-wallet": "0.6.0",
    "heap": "~0.2.6",
    "level-sublevel": "6.6.4",
    "levelup": "3.1.1",
    "lodash": "^4.17.10",
    "merkle-patricia-tree": "^2.2.0",
    "seedrandom": "~2.4.2",
    "tmp": "0.0.31",
    "web3": "^1.0.0-beta.35",
    "web3-provider-engine": "^14.0.6",
    "websocket": "^1.0.24"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.55",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "assert-match": "^1.1.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "cross-env": "^5.2.0",
    "js-scrypt": "^0.2.0",
    "lodash-webpack-plugin": "^0.11.5",
    "memdown": "^1.3.1",
    "mocha": "~5.1.1",
    "mocha-webpack": "^2.0.0-beta.0",
    "pify": "^3.0.0",
    "solc": "0.4.24",
    "temp": "^0.8.3",
    "webpack": "^4.16.4",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-cli": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trufflesuite/ganache-core"
  },
  "license": "MIT"
}
